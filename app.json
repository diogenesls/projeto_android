{
  "expo": {
    "name": "MANIAC",
    "slug": "MANIAC",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/man.png",
    "scheme": "maniac",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/man.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/man.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.koreanoair.MANIAC"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/man.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c8c95f46-f452-4e30-a8f4-25e90e7731bb"
      }
    }
  }
}
